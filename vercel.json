{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }